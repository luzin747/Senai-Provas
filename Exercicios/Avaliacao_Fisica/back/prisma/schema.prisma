// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Paciente {
  id_paciente Int @id @default(autoincrement())
  nome_completo String 
  nascimento String
  peso String
  altura String
}